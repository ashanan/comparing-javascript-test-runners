{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "prefer-arrow-callback": 0,
    "prettier/prettier": "error"
  },
  "env": {
    "mocha": true,
    "node": true
  }
}
